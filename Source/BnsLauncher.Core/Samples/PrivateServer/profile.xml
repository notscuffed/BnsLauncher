<?xml version="1.0" encoding="utf-8"?>
<profile>
    <!-- Look in launcher -->
    <name>Private Server</name>
    <background>red</background>
    <foreground>white</foreground>
    
    <!-- Client path & arguments -->
    <clientpath>.\bin\Client.exe</clientpath>
    <arguments>/LaunchByLauncher /LoginMode 2</arguments>

    <!-- Order priority in launcher, higher equals higher in the list.
         negative numbers allowed -->
    <priority>0</priority>
</profile>